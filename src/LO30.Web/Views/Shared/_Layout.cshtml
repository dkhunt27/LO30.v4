<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - LO30</title>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <!-- Add local styles, mostly for plugins css file -->
  @if (IsSectionDefined("Styles"))
      {@RenderSection("Styles", required: false)}

  <link rel="stylesheet" href="~/lib/eeh-navigation/dist/eeh-navigation.css" />

  <!-- FOLLOWING WILL NEED TO BE MIN'd -->
  <link rel="stylesheet" href="~/lib/slider-revolution/src/css/settings.css" />
  <link rel="stylesheet" href="~/lib/owl-carousel/owl-carousel/owl.carousel.css" />
  <link rel="stylesheet" href="~/lib/owl-carousel/owl-carousel/owl.theme.css" />
  <link rel="stylesheet" href="~/lib/owl-carousel/owl-carousel/owl.transitions.css" />
  <link rel="stylesheet" href="~/lib/angular-ui-select/dist/select.css" />

  <environment names="Development">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-select/dist/css/bootstrap-select.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/css/style.css" />
  </environment>
  <environment names="Staging,Production">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-select/dist/css/bootstrap-select.min.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
  </environment>


  <!-- for login modal -->
  


  @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
</head>
<body>
  <div id="modal-container" class="modal fade"
       tabindex="-1" role="dialog">
    <div class="modal-content">
    </div>
  </div>
  <div id="wrapper">
    <div id="top-nav" class="row">
      <!-- Top Navbar -->
      @Html.Partial("_TopNavbar")
    </div>

    <div id="page-plus-side-nav" class="row">
      <!-- Main view / Page  -->
      @RenderBody()
    </div>

    <div id="top-nav" class="row">
      @Html.Partial("_Footer")
    </div>
  </div>

  <environment names="Development">
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/bootstrap-select/dist/js/bootstrap-select.js"></script>
    <script src="~/lib/slimScroll/jquery.slimscroll.js"></script>
    <script src="~/lib/pace/pace.js"></script>
    <script src="~/js/script.js"></script>
  </environment>
  <environment names="Staging,Production">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="~/lib/pace/pace.min.js"></script>
    <script src="~/lib/slimScroll/jquery.slimscroll.min.js"></script>
    <script src="~/js/script.min.js" asp-append-version="true"></script>
  </environment>

  <script src="~/lib/angular/angular.js"></script>
  <script src="~/lib/angular-bootstrap-lightbox/dist/angular-bootstrap-lightbox.js"></script>


  <!-- FOLLOWING WILL NEED TO BE MIN'd -->
  <script src="~/lib/angular-animate/angular-animate.js"></script>
  <script src="~/lib/angular-route/angular-route.js"></script>
  <script src="~/lib/underscore/underscore.js"></script>
  <script src="~/lib/simple-js-validator/lib/simple.js.validator.js"></script>
  <script src="~/lib/moment/moment.js"></script>
  <script src="~/lib/angular-bootstrap/ui-bootstrap-tpls.js"></script>
  <script src="~/lib/angular-ui-select/dist/select.js"></script>
  <script src="~/lib/angular-sanitize/angular-sanitize.js"></script>
  <script src="~/lib/angular-media-queries/match-media.js"></script>
  <script src="~/lib/angular-ui-router/release/angular-ui-router.js"></script>
  <script src="~/lib/angular-translate/angular-translate.js"></script>


  <!-- FOLLOWING WILL NEED TO BE MIN'd -->
  <script src="~/lib/slider-revolution/src/js/jquery.themepunch.plugins.min.js"></script>
  <script src="~/lib/slider-revolution/src/js/jquery.themepunch.revolution.min.js"></script>
  <script src="~/lib/owl-carousel/owl-carousel/owl.carousel.min.js"></script>
  <script src="~/lib/eeh-navigation/dist/eeh-navigation.min.js"></script>
  <script src="~/lib/eeh-navigation/dist/eeh-navigation.tpl.min.js"></script>
  <script src="~/lib/angular-ui-select/dist/select.js"></script>


  @RenderSection("scripts", required: false)
</body>
</html>
